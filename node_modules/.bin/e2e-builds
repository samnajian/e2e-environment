#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/samnajian/Dev/woocommerce/packages/js/internal-e2e-builds/node_modules:/Users/samnajian/Dev/woocommerce/packages/js/node_modules:/Users/samnajian/Dev/woocommerce/packages/node_modules:/Users/samnajian/Dev/woocommerce/node_modules:/Users/samnajian/Dev/node_modules:/Users/samnajian/node_modules:/Users/node_modules:/node_modules:/Users/samnajian/Dev/woocommerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/samnajian/Dev/woocommerce/packages/js/internal-e2e-builds/node_modules:/Users/samnajian/Dev/woocommerce/packages/js/node_modules:/Users/samnajian/Dev/woocommerce/packages/node_modules:/Users/samnajian/Dev/woocommerce/node_modules:/Users/samnajian/Dev/node_modules:/Users/samnajian/node_modules:/Users/node_modules:/node_modules:/Users/samnajian/Dev/woocommerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@woocommerce/internal-e2e-builds/build.js" "$@"
else
  exec node  "$basedir/../@woocommerce/internal-e2e-builds/build.js" "$@"
fi
